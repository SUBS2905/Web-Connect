<!-- Header included -->
<%- include("./partials/header.ejs") %>

  
  <link rel="stylesheet" href="css/chat.css" />
<body>
  <a href="chatcordLogin" style="color: #32465a;"><i class="fas fa-arrow-left back"></i></a>
  <div id="frame" style="border-radius: 38px;">
    <div id="sidepanel" style="border-radius: 40px;">
      <div id="profile">
        <div class="wrap">
          <img id="profile-img" src="https://i.pinimg.com/originals/fd/14/a4/fd14a484f8e558209f0c2a94bc36b855.png"
            class="online" alt="" />
          <p id="owner-name">
            <%=name%>
          </p>
          <i class="fa fa-chevron-down expand-button" aria-hidden="true"></i>
          <div id="status-options">
            <ul>
              <li id="status-online" class="active">
                <span class="status-circle"></span>
                <p>Online</p>
              </li>
              <li id="status-away">
                <span class="status-circle"></span>
                <p>Away</p>
              </li>
              <li id="status-busy">
                <span class="status-circle"></span>
                <p>Busy</p>
              </li>
              <li id="status-offline">
                <span class="status-circle"></span>
                <p>Offline</p>
              </li>
            </ul>
          </div>
          <div id="expanded">
            <label for="twitter"><i class="fa fa-linkedin fa-fw" aria-hidden="true"></i></label>
            <input name="twitter" type="text" value=<%=linkedin%> />
            <label for="twitter"><i class="fa fa-github fa-fw" aria-hidden="true"></i></label>
            <input name="twitter" type="text" value=<%=github%> />
            <label for="twitter"><i class="fa fa-envelope fa-fw" aria-hidden="true"></i></label>
            <input name="twitter" type="text" value=<%=user%> />
          </div>
        </div>
      </div>
      <div id="contacts">
        <ul>
          <li class="contact">
            <div class="wrap" id="new">
              <span class="contact-status online"></span>
              <img
                src="https://media-exp1.licdn.com/dms/image/C5603AQFB-pVb8BSfbw/profile-displayphoto-shrink_800_800/0/1626953559559?e=1640822400&v=beta&t=NDkopNG5hN4ux6fc0HydhQjHYFSbvmv3fMmeGKFIlqQ"
                alt="" />
              <div class="meta" id="replace" onclick = "replace2()">
                <p class="name">Anant Dubey</p>
                <p class="preview">You just got LITT up, Mike.</p>
              </div>
            </div>
          </li>
          <li class="contact active">
            <div class="wrap" id="new2">
              <span class="contact-status busy"></span>
              
              <div class="meta" id="replace1" onclick = "replace1()"><img id="ansh img"src="https://miro.medium.com/fit/c/262/262/1*-TasmDvQZLqV_hz7LZef4Q.jpeg" alt="" />
                <p class="name">Ansh Chauhan</p>
                <p class="preview">
                  Wrong. You take the gun, or you pull out a bigger one. Or,
                  you call their bluff. Or, you do any one of a hundred and
                  forty six other things.
                </p>
              </div>
            </div>
          </li>
          <li class="contact">
            <div class="wrap" id="new1">
              <span class="contact-status away"></span>
              
              <div class="meta" id="replace2" onclick = "replace()"><img id="anu img"
                src="img/anurag.png"
          
                alt="" />
                <p class="name">Anurag Kumar</p>
                <p class="preview">
                  I was thinking that we could have chicken tonight, sounds
                  good?
                </p>
              </div>
            </div>
          </li>
          <li class="contact">
            <div class="wrap" id="new">
              <span class="contact-status online"></span>
              
              <div class="meta" id="replace3" onclick = "replace3()"><img
                src="img/subs.png" id="subs"
                alt="" />
                <p class="name">Subransu Majhi</p>
                <p class="preview">Mike, I know everything! I'm Donna..</p>
              </div>
            </div>
          </li>
        </ul>
      </div>
    </div>
    <div class="content">
      <div class="contact-profile" style="border-radius: 80px;">
        <img id="activem"src="https://miro.medium.com/fit/c/262/262/1*-TasmDvQZLqV_hz7LZef4Q.jpeg" alt="" />
        <p id="changei">Ansh Chauhan</p>
      </div>
      <div class="messages">
        <ul>
          <!-- <li class="sent">
            <img src="https://i.pinimg.com/originals/fd/14/a4/fd14a484f8e558209f0c2a94bc36b855.png" alt="" />
            <p>
            </p>
          </li> -->
          <!-- <li class="replies">
            <img src="https://miro.medium.com/fit/c/262/262/1*-TasmDvQZLqV_hz7LZef4Q.jpeg" alt="" />
            <p>
              Wrong. You take the gun, or you pull out a bigger one. Or, you
              call their bluff. Or, you do any one of a hundred and forty six
              other things.
            </p>
          </li> -->
        </ul>
      </div>
      <div class="message-input">
        <div class="wrap">
          <input style="border-radius: 80px;" type="text" placeholder="Write your message..." />
          <button style="border-radius: 80px;" class="submit">
            <i class="fa fa-paper-plane" aria-hidden="true"></i>
          </button>
        </div>
      </div>
    </div>
  </div>
  <div id="temp"></div>
  <script
    src="//production-assets.codepen.io/assets/common/stopExecutionOnTimeout-b2a7b3fe212eaa732349046d8416e00a9dec26eb7fd347590fbced3ab38af52e.js"></script>
  <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
  <script src="https://cdn.socket.io/4.3.2/socket.io.min.js" integrity="sha384-KAZ4DtjNhLChOB/hxXuKqhMLYvx3b5MlT55xPEiNmREKRzeEm+RVPlTnAn0ajQNs" crossorigin="anonymous"></script>
  <script src="js/chat.js"></script>
  
<!-- footer included -->
<%-include("./partials/footer.ejs") %>